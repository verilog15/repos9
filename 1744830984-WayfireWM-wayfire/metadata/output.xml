<?xml version="1.0"?>
<wayfire>
  <object name="output">
    <category>General</category>
    <option name="mode" type="output::mode">
      <default>auto</default>
    </option>
    <option name="position" type="output::position">
      <default>auto</default>
    </option>
    <option name="scale" type="double">
      <default>1.0</default>
    </option>
    <option name="transform" type="string">
      <default>normal</default>
    </option>
    <option name="vrr" type="bool">
      <default>false</default>
    </option>
    <option name="depth" type="int">
      <default>8</default>
      <min>8</min>
      <max>10</max>
    </option>
  </object>
</wayfire>
